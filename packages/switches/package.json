{
  "name": "@easyfeedback/switches",
  "version": "1.0.6",
  "description": "Switches are used as an alternative for the Checkbox component.",
  "keywords": [
    "easyfeedback"
  ],
  "homepage": "https://github.com/easyfeedback/RCL/tree/main/packages/switches#readme",
  "bugs": {
    "url": "https://github.com/easyfeedback/RCL/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easyfeedback/RCL.git"
  },
  "license": "MIT",
  "author": "Alexander BÃ¶hm",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "concurrently yarn:build:*",
    "build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist/cjs --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint": "concurrently yarn:lint:*",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path ../../config/.eslintignore",
    "lint:types": "tsc --noEmit",
    "test": "jest --env=jsdom --passWithNoTests",
    "test:ci": "jest --runInBand",
    "test:ci:cov": "yarn test:ci --coverage",
    "test:cov": "yarn test --coverage",
    "test:watch": "yarn test --watch -u"
  },
  "dependencies": {
    "@easyfeedback/test-utils": "^1.1.9",
    "@easyfeedback/utils": "^1.0.8",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.0",
    "react": "^16.13.0 || ^17.0.0",
    "react-dom": "^16.13.0 || ^17.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
