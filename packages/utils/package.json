{
  "name": "@easyfeedback/utils",
  "version": "1.0.1",
  "description": "Common utilities and types for the RCL components.",
  "keywords": [
    "easyfeedback",
    "hooks",
    "react",
    "utilities"
  ],
  "homepage": "https://github.com/easyfeedback/RCL#readme",
  "bugs": {
    "url": "https://github.com/easyfeedback/RCL/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easyfeedback/RCL.git"
  },
  "license": "MIT",
  "author": "Alexander BÃ¶hm",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "concurrently -n js,style,types 'yarn lint:js' 'yarn lint:style' 'yarn lint:types'",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path ../../config/.eslintignore",
    "lint:style": "stylelint '**/*.{css,scss,js,jsx,ts,tsx}' --ignore-path ../../config/.stylelintignore",
    "lint:types": "tsc --noEmit",
    "test": "jest --env=jsdom --passWithNoTests",
    "test:ci": "jest --runInBand",
    "test:ci:cov": "yarn test:ci --coverage",
    "test:cov": "yarn test --coverage",
    "test:watch": "yarn test --watch -u"
  },
  "devDependencies": {
    "@easyfeedback/test-utils": "workspace:^1.1.1",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^16.13.0 || ^17.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
